<?xml version="1.0"?>
<!--
    #############################################################################
     stab-indicator
    #############################################################################	  
    -->


<PropertyList>
 <path>stabind.ac</path>


<!-- Internal instruments lighting -->
<animation>
  <type>material</type>
  <object-name>disk1</object-name>
  <object-name>disk2</object-name>
  <object-name>rect</object-name>
</animation>
  
<!-- stab-indicator-->
<animation>
    <type>rotate</type>
    <object-name>rect</object-name>
    <property>/controls/flight/elevator-trim</property>
    <interpolation>
      <entry><ind>-0.70</ind>	<dep>45.0</dep></entry>
      <entry><ind>0.0</ind>	<dep>00.0</dep></entry>
      <entry><ind>0.70</ind>	<dep>-45.0</dep></entry>
    </interpolation>
		<center>
	   <x-m>0.000</x-m>
	   <y-m>-0.0123</y-m>
 		  <z-m>0.000</z-m>
		</center>
    <axis>
      <x>-1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
</animation>


</PropertyList>
